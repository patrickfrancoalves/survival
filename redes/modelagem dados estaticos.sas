%MACRO LOGIT(DEP);
ODS SELECT PARAMETERESTIMATES;
ODS OUTPUT PARAMETERESTIMATES=PARAM_&DEP.(DROP=DF StdErr WaldChiSq _ESTTYPE_);
PROC LOGISTIC DATA=SURVIV.DADOS_ESTATICOSX;
CLASS UF_NASCN GRANDE_AREAN SEXON VINCULON LOCAL_FORMACN;
MODEL &DEP.(REF=FIRST)=LN_IDADE LN_COAUTOR LN_LOC_TRAB LN_CAPES LOCAL_FORMACN
                       UF_NASCN GRANDE_AREAN DIRECAO SEXON VINCULON / MAXITER=80 LINK=logit LACKFIT RSQUARE;
OUTPUT OUT=SAIDA_&DEP.(KEEP=ArquivoXML ANP_ANP ANP_LATTES CENPES_LATTES PETRO_LAB PETRO_LATTES PETRO_PETRO P) PROB=P;
RUN;

%MEND LOGIT;
