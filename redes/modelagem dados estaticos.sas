/*PROC FREQ DATA=DADOS_ESTATICOS ORDER=FREQ;TABLE PETRO /NOCOL NOPERCENT NOROW;RUN;*/
/*ANP_ANP ANP_LATTES CENPES_LATTES PETRO_LAB PETRO_LATTES PETRO_PETRO*/
PROC FREQ DATA=DADOS_ESTATICOS ORDER=FREQ NOPRINT;
where CENPES_LATTES=1 OR PETRO_PETRO=1 OR PETRO_lattes=1;
TABLE VINCULO /NOCOL NOPERCENT NOROW OUT=TEMP;
RUN;


%MACRO FREQ(TRAT,CATEG);
PROC SQL NOPRINT;
CREATE TABLE TEMP AS 
SELECT &CATEG.,&TRAT.,COUNT(*) AS TOTAL 
FROM DADOS_ESTATICOS
WHERE GRANDE_AREA IN ('CIENCIAS_EXATAS_E_DA_TERRA','ENGENHARIAS','OUTROS')
GROUP BY &CATEG.,&TRAT.
ORDER BY &CATEG.;
QUIT;

PROC TRANSPOSE DATA=TEMP OUT=TEMP(DROP=_NAME_ _LABEL_) PREFIX=TRAT_;
BY &CATEG.;
ID &TRAT.;
VAR TOTAL;
RUN;
%MEND FREQ;
%FREQ(ANP_ANP,LOCAL_FORMAC);


