/*--                   REGIÃO DE SUPORTE COMUM                       --*/
/*---------------------------------------------------------------------*/

%MACRO PREP(DEP);
PROC MEANS DATA=SAIDA_&DEP. NOPRINT NWAY;
CLASS &DEP.;
OUTPUT OUT=SUP_&DEP.(DROP=_FREQ_ _type_) P25(P)=PROBINF P75(P)=PROBSUP;
RUN;

PROC SQL;
CREATE TABLE SAIDA_&DEP. AS 
SELECT A.*,B.PROBSUP AS PROBSUP0,C.PROBINF AS PROBINF1
FROM SAIDA_&DEP. AS A 
CROSS JOIN SUP_&DEP. AS B 
CROSS JOIN SUP_&DEP. AS C 
WHERE B.&DEP.=0 AND C.&DEP.=1;
QUIT;

PROC SQL;
CREATE TABLE SAIDAX_&DEP. AS 
SELECT DISTINCT A.ArquivoXML,A.&DEP.,
       CASE WHEN A.&DEP.=1 THEN 1/P ELSE 1/(1-P) END AS PESO,
	   CASE WHEN A.&DEP.=1 THEN 1   ELSE P/(1-P) END AS PESOX,
       B.LN_IDADE,B.LN_COAUTOR,B.LN_LOC_TRAB,B.LN_CAPES,B.DIRECAO,
	   B.UF_NASCN,B.GRANDE_AREAN,B.SEXON,B.VINCULON,B.LOCAL_FORMACN,
	   B.LN_ARTG_A1_A2       ,
       B.LN_ARTG_B1_B2       ,
       B.LN_ARTG_B3_B4       ,
       B.LN_ARTG_B5_C        ,
       B.LN_ARTG_AGRARI      ,
       B.LN_ARTG_BIOLOG      ,
       B.LN_ARTG_C_EXAT      ,
       B.LN_ARTG_ENGENH      ,
	   B.LN_ARTG_LETRAS      ,
	   B.LN_ARTG_C_HUMA      ,
	   B.LN_ARTG_C_SAUD      ,
	   B.LN_ARTG_OUTROS      ,
	   B.LN_ARTG_SOC_AP      ,
       B.LN_ARTIGOS          ,
	   B.LN_ARTIGO_BRASIL    ,
       B.LN_ARTIGO_EXTERIOR  ,
       B.LN_LIVRO_BRASIL     ,
       B.LN_LIVRO_EXTERIOR   ,
       B.LN_LIVRO            ,
       B.LN_PATENTE_BRASIL   ,
       B.LN_PATENTE_EXTERIOR ,
       B.LN_PATENTE          

FROM SAIDA_&DEP. AS A
LEFT JOIN SURVIV.DADOS_ESTATICOSX AS B ON A.ArquivoXML=B.ArquivoXML
WHERE P<=PROBSUP0 AND P>=PROBINF1;
QUIT;

%MEND PREP;
%PREP(ANP_ANP);
%PREP(ANP_LATTES);
%PREP(CENPES_LATTES);
%PREP(PETRO_LAB);
%PREP(PETRO_LATTES);
